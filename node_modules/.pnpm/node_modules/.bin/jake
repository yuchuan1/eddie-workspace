#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/eddie/eddie/eddie-workspace/node_modules/.pnpm/jake@10.9.4/node_modules/jake/bin/node_modules:/Users/eddie/eddie/eddie-workspace/node_modules/.pnpm/jake@10.9.4/node_modules/jake/node_modules:/Users/eddie/eddie/eddie-workspace/node_modules/.pnpm/jake@10.9.4/node_modules:/Users/eddie/eddie/eddie-workspace/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/eddie/eddie/eddie-workspace/node_modules/.pnpm/jake@10.9.4/node_modules/jake/bin/node_modules:/Users/eddie/eddie/eddie-workspace/node_modules/.pnpm/jake@10.9.4/node_modules/jake/node_modules:/Users/eddie/eddie/eddie-workspace/node_modules/.pnpm/jake@10.9.4/node_modules:/Users/eddie/eddie/eddie-workspace/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../jake/bin/cli.js" "$@"
else
  exec node  "$basedir/../jake/bin/cli.js" "$@"
fi
