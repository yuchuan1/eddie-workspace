#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/eddie/eddie/eddie-workspace/node_modules/.pnpm/@module-federation+cli@0.21.3_typescript@5.9.3/node_modules/@module-federation/cli/bin/node_modules:/Users/eddie/eddie/eddie-workspace/node_modules/.pnpm/@module-federation+cli@0.21.3_typescript@5.9.3/node_modules/@module-federation/cli/node_modules:/Users/eddie/eddie/eddie-workspace/node_modules/.pnpm/@module-federation+cli@0.21.3_typescript@5.9.3/node_modules/@module-federation/node_modules:/Users/eddie/eddie/eddie-workspace/node_modules/.pnpm/@module-federation+cli@0.21.3_typescript@5.9.3/node_modules:/Users/eddie/eddie/eddie-workspace/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/eddie/eddie/eddie-workspace/node_modules/.pnpm/@module-federation+cli@0.21.3_typescript@5.9.3/node_modules/@module-federation/cli/bin/node_modules:/Users/eddie/eddie/eddie-workspace/node_modules/.pnpm/@module-federation+cli@0.21.3_typescript@5.9.3/node_modules/@module-federation/cli/node_modules:/Users/eddie/eddie/eddie-workspace/node_modules/.pnpm/@module-federation+cli@0.21.3_typescript@5.9.3/node_modules/@module-federation/node_modules:/Users/eddie/eddie/eddie-workspace/node_modules/.pnpm/@module-federation+cli@0.21.3_typescript@5.9.3/node_modules:/Users/eddie/eddie/eddie-workspace/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../cli/bin/mf.js" "$@"
else
  exec node  "$basedir/../../../cli/bin/mf.js" "$@"
fi
