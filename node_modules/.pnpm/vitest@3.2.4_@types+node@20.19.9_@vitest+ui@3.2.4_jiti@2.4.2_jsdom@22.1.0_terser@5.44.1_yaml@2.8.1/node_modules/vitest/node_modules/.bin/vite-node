#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/eddie/eddie/eddie-workspace/node_modules/.pnpm/vite-node@3.2.4_@types+node@20.19.9_jiti@2.4.2_terser@5.44.1_yaml@2.8.1/node_modules/vite-node/node_modules:/Users/eddie/eddie/eddie-workspace/node_modules/.pnpm/vite-node@3.2.4_@types+node@20.19.9_jiti@2.4.2_terser@5.44.1_yaml@2.8.1/node_modules:/Users/eddie/eddie/eddie-workspace/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/eddie/eddie/eddie-workspace/node_modules/.pnpm/vite-node@3.2.4_@types+node@20.19.9_jiti@2.4.2_terser@5.44.1_yaml@2.8.1/node_modules/vite-node/node_modules:/Users/eddie/eddie/eddie-workspace/node_modules/.pnpm/vite-node@3.2.4_@types+node@20.19.9_jiti@2.4.2_terser@5.44.1_yaml@2.8.1/node_modules:/Users/eddie/eddie/eddie-workspace/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../vite-node/vite-node.mjs" "$@"
else
  exec node  "$basedir/../../../vite-node/vite-node.mjs" "$@"
fi
