{
  "name": "react-code-editor",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/react-code-editor/src",
  "projectType": "library",
  "tags": ["scope:shared", "type:feature"],
  "// targets": "to see all targets run: nx show project react-code-editor --web",
  "targets": {
    "test": {
      "executor": "@nx/vite:test",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "config": "libs/react-code-editor/vite.config.ts"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "storybook": {
      "executor": "@nx/storybook:storybook",
      "options": {
        "configDir": "libs/react-code-editor/.storybook",
        "port": 6008
      }
    },
    "build-storybook": {
      "executor": "@nx/storybook:build",
      "outputs": ["{workspaceRoot}/dist/storybook/libs/react-code-editor"],
      "options": {
        "configDir": "libs/react-code-editor/.storybook",
        "outputDir": "dist/storybook/libs/react-code-editor"
      }
    }
  }
}
