{
  "name": "react-media-player",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/react-media-player/src",
  "projectType": "library",
  "tags": ["scope:shared", "type:feature"],
  "// targets": "to see all targets run: nx show project react-media-player --web",
  "targets": {
    "storybook": {
      "executor": "@nx/storybook:storybook",
      "options": {
        "configDir": "libs/react-media-player/.storybook",
        "port": 6007
      }
    },
    "build-storybook": {
      "executor": "@nx/storybook:build",
      "outputs": ["{workspaceRoot}/dist/storybook/libs/react-media-player"],
      "options": {
        "configDir": "libs/react-media-player/.storybook",
        "outputDir": "dist/storybook/libs/react-media-player"
      }
    }
  }
}
